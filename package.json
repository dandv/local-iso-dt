{
  "name": "local-iso-dt",
  "version": "2.0.0",
  "description": "Convert the most common types of dates into ISO8601/RFC3339-formatted YYYY-MM-DDTHH:MM:SS strings in the local timezone",
  "keywords": [
    "datetime",
    "date",
    "time",
    "format",
    "string",
    "convert",
    "local",
    "timezone",
    "ISO8601",
    "RFC3339"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/dandv/local-iso-dt.git"
  },
  "license": "MIT",
  "author": {
    "name": "Dan Dascalescu",
    "url": "https://dandascalescu.com/about"
  },
  "type": "commonjs",
  "main": "index.mjs",
  "files": [
    "index.*"
  ],
  "scripts": {
    "lint": "eslint index*.mjs",
    "prepare": "babel index.mjs --out-file index.js && babel index.mjs --minified --no-comments --source-maps -o index.min.js",
    "test": "jest"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.7",
    "@babel/plugin-transform-modules-commonjs": "^7.8.3",
    "@types/jest": "^25.1.3",
    "eslint": "^6.8.0",
    "eslint-plugin-jest": "^23.8.2",
    "jest": "^25.1.0"
  }
}
